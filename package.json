{
  "name": "weather-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mock:server": "npm run concat:mocks && json-server --routes routes.json --watch mocks/data.json",
